{
    "name": "laradic/config",
    "description": "Laravel 5 config package",
    "homepage": "https://github.com/laradic/config",
    "keywords": [
        "config",
        "persistent",
        "namespace",
        "laravel"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Robin Radic",
            "email": "robin@radic.nl",
            "role": "Package Author/Developer"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=5.4.0",
        "laradic/support": "dev-master"
    },
    "require-dev": {
        "laradic/dev": "dev-master",
        "laradic/debug": "dev-master",
        "laradic/support": "dev-master",
        "gooh/interface-distiller": "dev-master",
        "radic/tmp-underscore-php": "dev-master"

    },
    "scripts": {
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "Laradic\\Config\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Laradic\\Tests\\Config\\": "tests/"
        }
    }
}
